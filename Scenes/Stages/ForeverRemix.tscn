[sub_resource type="GDScript" id=18]
script/source = "extends Sprite

export (NodePath) var copy_path
onready var copy_node = get_node(copy_path)

export (bool) var activated = false
export (float) var speed = 1
export (float) var move_speed = 1 

var direction = 1

func _physics_process(delta):
	position.x += move_speed * direction * delta
	if position.x < 900:
		position.x = 900
		direction = 1
	if position.x > 950:
		position.x = 950
		direction = -1
	
	activated = copy_node.activated
	speed = copy_node.speed
	if activated:
		position.y += delta * speed
		if position.y > 536 * scale.y:
			position.y = 280 * scale.y
"

