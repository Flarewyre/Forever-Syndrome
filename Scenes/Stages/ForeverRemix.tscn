script/source = "extends TileMap

export (float) var speed = 1.0
var time = 0

func _physics_process(delta):
	speed = get_parent().get_node(\"Ground\").speed / 3
	
	position.x += delta * speed
	if position.x > 64 * scale.x:
		position.x = 0
	
	position.y = (get_parent().get_node(\"Ground\").position.y / 2) + 64
"

