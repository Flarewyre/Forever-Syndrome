[sub_resource type="GDScript" id=14]
script/source = "extends Node2D

export(NodePath) var current_scene_path
onready var current_scene = get_node(current_scene_path)

export var activated = false
export var target_pos : float

export var speed : float

var og_pos : Vector2
var relative_pos0 : Vector2
var relative_pos1 : Vector2

var returning = false
var wait_timer = 0.0

var mario
var bf

func _ready():
	for i in range(5):
		yield(get_tree(), \"physics_frame\")
	og_pos = $Platform.global_position
	
	mario = current_scene.get_node(\"Characters/Mario Forever\")
	bf = current_scene.get_node(\"Characters/Boyfriend Forever\")
	
	relative_pos0 = og_pos - mario.global_position
	relative_pos1 = og_pos - bf.global_position

func _physics_process(delta):
	if wait_timer > 0:
		wait_timer -= delta
		if wait_timer <= 0:
			wait_timer = 0
			returning = true
	
	if activated && !returning:
		$Platform.global_position.y -= speed * delta
		speed += 6
		
		if $Platform.global_position.y < target_pos:
			$Platform.global_position.y = 1200
			wait_timer = 1.5
			activated = false

		mario.global_position.y = $Platform.global_position.y - relative_pos0.y
		bf.global_position.y = $Platform.global_position.y - relative_pos1.y
	elif returning:
		$Platform.global_position.y = lerp($Platform.global_position.y, og_pos.y, delta)
		$Platform.global_position.y -= 20 * delta
		
		if $Platform.global_position.y <= og_pos.y:
			returning = false
			speed = 0
			$Platform.global_position.y = og_pos.y

		mario.global_position.y = $Platform.global_position.y - relative_pos0.y
		bf.global_position.y = $Platform.global_position.y - relative_pos1.y
"

[sub_resource type="GDScript" id=15]
script/source = "extends TileMap

export (NodePath) var copy_path
onready var copy_node = get_node(copy_path)

export (bool) var activated = false
export (float) var speed = 1 

func _physics_process(delta):
	activated = copy_node.activated
	speed = copy_node.speed / 2
	if activated:
		position.y += delta * speed
		if position.y > 536 * scale.y:
			position.y = 280 * scale.y
"

